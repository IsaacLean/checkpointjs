{
  "name": "checkpointjs",
  "version": "0.0.4",
  "description": "Validate and transform data",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "tsc",
    "format": "prettier --write --parser=typescript --print-width=120 --semi=false --single-quote \"src/**/*.{js,ts}\"",
    "lint": "eslint --cache \"src/**/*.{js,ts}\"",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint && npm t",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:update": "jest -u",
    "test:watch": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/IsaacLean/checkpointjs.git"
  },
  "keywords": [
    "checkpointjs",
    "checkpoint",
    "checkpoint.js",
    "data",
    "validate",
    "validation",
    "transform",
    "transformation",
    "sanitize",
    "sanitization"
  ],
  "author": "Matthew Lean",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/IsaacLean/checkpointjs/issues"
  },
  "homepage": "https://github.com/IsaacLean/checkpointjs#readme",
  "dependencies": {
    "lodash": "^4.17.11",
    "validator": "^10.11.0"
  },
  "peerDependencies": {
    "lodash": "^4.17.11",
    "validator": "^10.11.0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.6",
    "@types/node": "^11.9.5",
    "@types/validator": "^10.9.0",
    "@typescript-eslint/eslint-plugin": "^1.4.0",
    "@typescript-eslint/parser": "^1.4.0",
    "eslint": "^5.14.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jest": "^22.3.0",
    "eslint-plugin-prettier": "^3.0.1",
    "jest": "^24.1.0",
    "prettier": "^1.16.4",
    "ts-jest": "^24.0.0",
    "typescript": "^3.3.3333"
  }
}
